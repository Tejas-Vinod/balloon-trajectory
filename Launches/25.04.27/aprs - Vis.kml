<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.1">
<Folder>
  <name>APRS tracking by aprs.fi</name>

  <!-- Orange style -->
  <Style id="orangeStyle">
    <LineStyle>
      <color>7f007fff</color> <!-- Orange line -->
      <width>4</width>
    </LineStyle>
    <PolyStyle>
      <color>7f007f00</color> <!-- Orange-ish polygon fill -->
    </PolyStyle>
  </Style>

  <!-- Dummy placemark using the orange style -->
  <Placemark>
    <name>Style Preview</name>
    <styleUrl>#orangeStyle</styleUrl>
    <LineString>
      <coordinates>
        -80.0,35.0,0
        -80.1,35.1,0
      </coordinates>
    </LineString>
  </Placemark>

  <NetworkLink>
    <visibility>1</visibility>
    <name>APRS tracking: KJ4OAP-11</name>
    <description><![CDATA[
      <p>Tracking: KJ4OAP-11</p>
      <p>Google Earth application by <a href='http://aprs.fi/'>aprs.fi</a></p>
    ]]></description>
    <Snippet maxLines='2'>Tracking KJ4OAP-11</Snippet>
    <Link>
      <href>http://aprs.fi/aprsupdate.kml?units=metric&amp;units_temp=C&amp;call=KJ4OAP-11</href>
      <viewRefreshMode>onStop</viewRefreshMode>
      <viewRefreshTime>1</viewRefreshTime>
      <refreshMode>onExpire</refreshMode>
    </Link>
  </NetworkLink>

  <NetworkLink>
    <visibility>0</visibility>
    <name>APRS tracking of all targets</name>
    <description><![CDATA[
      Real-time APRS network tracking of all APRS targets in the world
      <p>Google Earth application by <a href='http://aprs.fi/'>aprs.fi</a></p>
    ]]></description>
    <Snippet maxLines='2'>Real-time APRS network tracking of all APRS targets in the world</Snippet>
    <Link>
      <href>http://aprs.fi/aprsupdate.kml?units=metric&amp;units_temp=C</href>
      <viewRefreshMode>onStop</viewRefreshMode>
      <viewRefreshTime>1</viewRefreshTime>
      <refreshMode>onExpire</refreshMode>
    </Link>
  </NetworkLink>
</Folder>
</kml>
